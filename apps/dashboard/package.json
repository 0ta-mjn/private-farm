{
  "name": "@repo/dashboard",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -H 127.0.0.1",
    "build": "next build",
    "start": "next start",
    "start:testing": "dotenv -e .env.testing -- next build && next start",
    "lint": "next lint --max-warnings 0",
    "test": "pnpm test:unit",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui --watch",
    "typecheck": "tsc --noEmit",
    "cf:build": "opennextjs-cloudflare build --skipWranglerConfigCheck",
    "cf:preview": "opennextjs-cloudflare preview -- --config ../../infra/cloudflare/dashboard/wrangler.toml --port 3000 --host 127.0.0.1",
    "cf:deploy": "pnpm cf:build && opennextjs-cloudflare deploy -- --config ../../infra/cloudflare/dashboard/wrangler.toml",
    "cf:upload": "pnpm cf:build && opennextjs-cloudflare upload -- --config ../../infra/cloudflare/dashboard/wrangler.toml",
    "cf:typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts --config ../../infra/cloudflare/dashboard/wrangler.toml"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@opennextjs/cloudflare": "file:fork/opennextjs-cloudflare-1.3.1.tgz",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@supabase/supabase-js": "catalog:",
    "@tanstack/react-query": "catalog:",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "hono": "catalog:",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "9.7.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "sonner": "^2.0.5",
    "superjson": "catalog:",
    "tailwind-merge": "^3.3.0",
    "vaul": "^1.1.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/api": "workspace:*",
    "@repo/config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "catalog:",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/ui": "3.2.1",
    "eslint": "catalog:",
    "happy-dom": "^17.6.3",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.3",
    "typescript": "catalog:",
