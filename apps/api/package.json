{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "dotenv -e .env -- tsup --watch --onSuccess \"node dist/server.cjs\"",
    "start": "dotenv -e .env node dist/server.cjs",
    "start:testing": "pnpm build && dotenv -e .env.testing  tsx src/server.ts",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@trpc/server": "catalog:",
    "fastify": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "catalog:",
    "@repo/api": "workspace:*",
    "@repo/db": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/supabase": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
