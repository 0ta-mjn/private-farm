{
  "name": "@repo/sensor-ingest",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --noEmit && tsup",
    "dev": "wrangler dev src/producer/index.ts --config ../../infra/cloudflare/sensor-ingest/producer/wrangler.toml --config ../../infra/cloudflare/sensor-ingest/consumer/wrangler.toml --port 8101 --ip 0.0.0.0 --persist-to ../../infra/cloudflare/sensor-ingest/.wrangler/state",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "test": "dotenv -e .env.testing -- vitest",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "cf:generate:types": "wrangler types --config ../../infra/cloudflare/sensor-ingest/producer/wrangler.toml --config ../../infra/cloudflare/sensor-ingest/consumer/wrangler.toml"
  },
  "dependencies": {
    "@chirpstack/chirpstack-api": "^4.13.0",
    "hono": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/dashboard-db": "workspace:*",
    "@repo/discord": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/sensor-db": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "wrangler": "catalog:"
  }
}
