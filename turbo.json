{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "!.cache/**"],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_API_URL",
        "ACCEPT_ORIGINS",
        "SUPABASE_URL",
        "SUPABASE_KEY",
        "DATABASE_URL",
        "API_PORT",
        "DASHBOARD_PORT",
        "CI"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": ["lint"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "env": [
        "DATABASE_URL",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_API_URL",
        "CI"
      ]
    },
    "e2e": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_API_URL",
        "NEXT_PUBLIC_SITE_URL",
        "ACCEPT_ORIGINS",
        "SUPABASE_URL",
        "SUPABASE_KEY",
        "DATABASE_URL",
        "API_PORT",
        "DASHBOARD_PORT",
        "SUPABASE_ANON_KEY",
        "SUPABASE_INBUCKET_URL",
        "CI"
      ],
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "cache": false,
      "persistent": true,
      "env": [
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_API_URL",
        "NEXT_PUBLIC_SITE_URL",
        "ACCEPT_ORIGINS",
        "SUPABASE_URL",
        "SUPABASE_KEY",
        "DATABASE_URL",
        "API_PORT",
        "DASHBOARD_PORT"
      ]
    },
    "push": {
      "dependsOn": ["^push"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "env": ["DATABASE_URL"],
      "cache": false
    },
    "stop": {
      "cache": false
    },
    "stopd": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    }
  },
  "globalPassThroughEnv": ["NODE_ENV", "PORT", "CI"]
}
