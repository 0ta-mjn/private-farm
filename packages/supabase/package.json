{
  "name": "@repo/supabase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint & supabase db lint",
    "setup": "supabase status || supabase start",
    "reset": "supabase db reset || supabase start",
    "stop": "supabase stop",
    "stopd": "supabase stop --no-backup",
    "deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@supabase/supabase-js": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "supabase": "^2.26.9",
    "typescript": "catalog:"
  }
}
